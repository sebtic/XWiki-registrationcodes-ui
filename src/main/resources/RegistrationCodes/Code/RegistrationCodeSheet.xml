{{html wiki=true}}
#set($docextras=[])
$xwiki.jsx.use("RegistrationCodes.Code.RegistrationCodeSheet")
$xwiki.ssx.use("RegistrationCodes.Code.RegistrationCodeSheet")
### setting right mode for the sheet 
### setting $curdoc variable either already set or set to $doc
#if(!$mode)
  #if($xcontext.action=="inline" || $xcontext.action=="edit")
    #set($mode = "edit")
  #else
    #set($mode = "view")
  #end
#end
#if(!$curdoc)
  #set($curdoc = $doc)
#end
$xwiki.jsx.use("RegistrationCodes.Code.RegistrationCodeSheet")
$xwiki.ssx.use("RegistrationCodes.Code.RegistrationCodeSheet")
#set($xobject = $curdoc.getObject('RegistrationCodes.Code.RegistrationCodeClass'))
#### begin display macros
### this macro shows a validation error message if it exists
#macro(showvalidationmessage $message)
  #if($xcontext.validationStatus.errors.contains($message))
    {{error}}$services.localization.render($message){{/error}}
  #end
#end ## end showvalidationmessage
### this macros displays a field and it's tooltip
#macro(showfield $fieldname $mode)
  #if( $mode == "edit")
    (% fieldname="$fieldname" %) (((
      &lt;label for="Bibliography.Code.PersonClass_0_$fieldname"
        data-toggle='tooltip' 
        title='$services.localization.render("registrationcodes.tooltip_$fieldname")'&gt;
          $services.localization.render("registrationcodes.label_$fieldname")
          &lt;span class="tooltip-mark"&gt;$services.icon.render('question','Silk')&lt;/span&gt; :
      &lt;/label&gt;
      $curdoc.display($fieldname, $mode)
     #showvalidationmessage("registrationcodes.val_$fieldname")
    )))
  #else
    #set($value = $xobject.getProperty($fieldname).value)
    ##if ($stringtool.isNotBlank($value.trim()))
      (% fieldname="$fieldname" %) (((
        &lt;span data-toggle='tooltip' 
          title='$services.localization.render("registrationcodes.tooltip_$fieldname")'&gt; 
          $services.localization.render("registrationcodes.label_$fieldname")
          &lt;span class="tooltip-mark"&gt;$services.icon.render('question','Silk')&lt;/span&gt;
        &lt;/span&gt; : $curdoc.display($fieldname, $mode)
      )))
    ##end
  #end
#end ## end showfield macro
##### end macros
$curdoc.use("RegistrationCodes.Code.RegistrationCodeClass")
(((

#showfield("code", 'view')
#showfield("active", $mode)
#showfield("description", $mode)
#showfield("startDate", $mode)
#showfield("endDate", $mode)
#showfield("maxUse", $mode)
#showfield("addToWikis", $mode)
#showfield("addToGroups", $mode)

#if($mode != "edit")
#showfield("lastUsed", 'view')
#showfield("users", 'view')
#end

)))

## Invoke validation
&lt;input type="hidden" name="xvalidate" value="1" /&gt;
## csrf
&lt;input type="hidden" name="form_token" value="$!{services.csrf.getToken()}" /&gt;
{{/html}}
{{/velocity}}</content>
